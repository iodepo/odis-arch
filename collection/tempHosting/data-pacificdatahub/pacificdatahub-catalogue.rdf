            geosparql:asWKT "POLYGON((141.4544677734375 -0.1235960955504786, 141.4544677734375 11.401956477722216, 164.8333740234375 11.401956477722216, 164.8333740234375 -0.1235960955504786, 141.4544677734375 -0.1235960955504786))"^^geosparql:wktLiteral ;
