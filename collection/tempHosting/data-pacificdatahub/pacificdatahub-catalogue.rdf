<https://pacificdata.org/data/dataset/tuvalu-tide-and-sea-level-information-1> a :Dataset ;
    geosparql:hasGeometry [ a <http://www.opengis.net/ont/sf#Polygon> ;
            geosparql:asWKT "POLYGON((172.71256944444497 -13.240386111110979, 172.71256944444497 -3.965563888888767, 183.243352777778 -3.965563888888767, 183.243352777778 -13.240386111110979, 172.71256944444497 -13.240386111110979))"^^geosparql:wktLiteral ;
